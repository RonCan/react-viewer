{
  "npm": {
    "publish": true,
    "publishPath": "lib"
  },
  "dry-run": true,
  "requireCleanWorkingDir": false,
  "src": {
    "afterReleaseCommand": "cp -rf package.json README.md CHANGELOG.md lib"
  },
  "buildCommand": "node ./script/release.js build",
  "dist": {
    "afterReleaseCommand": "node ./script/release.js released"
  }
}
